<% include ./partials/header %>




<div class="row">
    <div class="col-lg-12">
        <h1 id="formTitle"> Register!</h1>
        <div id="formDiv">
            <form action="/register" method="POST">
                <div class="form-group">
                    <input type="text" placeholder="username" name="username" class="form-control"></input>
                </div>
                <div class="form-group">
                    <input type="password" placeholder="password" name="password" class="form-control"></input>
                </div>
                <div class="form-group">
                    <input type="password" placeholder="Privilege Code" name="privilegeCode" class="form-control"></input>
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Sign Up!</button>
                </div>
            </form>
             <a href="/">Go Back</a>
        </div>
    </div>
</div>




<% include ./partials/footer %>